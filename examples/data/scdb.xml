  <rdfs:domain rdf:resource="#Lesion"/>
  <rdfs:range rdf:resource="#Concept"/>
  <owl:inverseOf rdf:resource="#isConceptOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isConceptOf">
